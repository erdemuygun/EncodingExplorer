<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="EncodingExplorer">
  <schema id="io.github.josephmawa.EncodingExplorer" path="/io/github/josephmawa/EncodingExplorer/">
    <key name="window-width" type="i">
      <default>800</default>
    </key>
    <key name="window-height" type="i">
      <default>600</default>
    </key>
    <key name="window-maximized" type="b">
      <default>false</default>
    </key>
    <key name="preferred-theme" type="s">
      <choices>
        <choice value="system" />
        <choice value="dark" />
        <choice value="light" />
      </choices>
      <default>"system"</default>
    </key>
    <key name="endianness" type="s">
      <choices>
        <choice value="le" />
        <choice value="be" />
      </choices>
      <default>"le"</default>
    </key>
    <key name="encoding-mode" type="s">
      <choices>
        <choice value="text_encoding" />
        <choice value="ieee_754" />
      </choices>
      <default>"text_encoding"</default>
    </key>
    <key name="radix" type="s">
      <choices>
        <choice value="Binary" />
        <choice value="Octal" />
        <choice value="Decimal" />
        <choice value="Hexadecimal" />
      </choices>
      <default>"Binary"</default>
    </key>
    <key name="encoding" type="s">
      <choices>
        <choice value="ASCII" />
        <choice value="UTF-8" />
        <choice value="UTF-16" />
        <choice value="UTF-32" />
        <choice value="UCS-4" />
      </choices>
      <default>"UTF-8"</default>
    </key>
  </schema>
</schemalist>